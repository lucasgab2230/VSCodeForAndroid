{
  "name": "vscode-android",
  "displayName": "VS Code Android",
  "version": "1.0.0",
  "description": "Visual Studio Code for Android with Termux integration",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "build:vscode-core": "cd vscode-core && npm run build",
    "build:termux-integration": "cd termux-integration && npm run build",
    "clean": "rm -rf node_modules && rm -rf android/app/build && rm -rf ios/build",
    "setup": "npm install && npm run build:vscode-core && npm run build:termux-integration"
  },
  "dependencies": {
    "expo": "~50.0.0",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "@expo/vector-icons": "^14.0.0",
    "react-native-webview": "^13.8.0",
    "react-native-terminal": "^0.2.0",
    "react-native-fs": "^2.20.0",
    "vscode-core": "file:./vscode-core",
    "termux-integration": "file:./termux-integration"
  },
  "devDependencies": {
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.73.0",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0"
  },
  "keywords": [
    "vscode",
    "android",
    "termux",
    "react-native",
    "expo",
    "mobile-development"
  ],
  "author": "VS Code Android Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/vscode-android.git"
  }
}